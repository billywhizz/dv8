{
  "CC": "ccache g++",
  "deps": "/deps",
  "src": "/src",
  "build": "/build",
  "target": "release",
  "override": false,
  "builtins": false,
  "output": {
    "build": "./build/platform.sh",
    "modulesHeader": "./src/modules.h",
    "modulesSource": "./src/modules.cc"
  },
  "libs": [
    "/usr/lib/x86_64-linux-gnu/libssl.a",
    "/usr/lib/x86_64-linux-gnu/libcrypto.a",
    "/deps/mbedtls/library/libmbedcrypto.a"
  ],
  "modules": [
    "loop",
    "timer",
    "fs",
    "process",
    "tty",
    "libz",
    "os",
    "socket",
    "thread",
    "udp",
    "mbedtls",
    "httpParser",
    "openssl"
  ]
}
